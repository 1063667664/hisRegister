<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.hisRegister.entity.Doctor" table="doctor_info">
		<id name="id" type="int">
			<column name="id" />
			<!-- <generator class="assigned"></generator> -->
		</id>
		<property name="doctorName" type="java.lang.String">
			<column name="doctorName" length="30" not-null="true"></column>
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="30" not-null="true"></column>
		</property>
	</class>
</hibernate-mapping>